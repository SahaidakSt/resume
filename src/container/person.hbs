<main class="p-5">
  {{#with person}}
  <div class='row p-5'>
    <div class='col-12 '>
      <div class='card mb-5'>
        <div class='card-body'>
          <h1 class='card-title'>{{name}}</h1>
          <section class="my-5">
            <div>Age: {{age}}</div>
            <div>{{gender}}</div>
            {{#with address}}
            <address>
              {{street}},
              {{city}},
              {{state}},
              {{zip}},
              {{country}}
            </address>
            {{/with}}
          </section>
          <hr />
          <section class="my-5">
            {{>title data='Educations' big=true}}
            <ul>
              {{#each education}}
              <li>
                <div>{{degree}}</div>
                <div>{{major}}</div>
                <div>{{university}}
                  ({{graduationYear}})</div>
              </li>
              {{/each}}
            </ul>
          </section>
          <hr />
          <section class="my-5">
            {{>title data='Work experience' big=true}}
            <ul>
              {{#each workExperience}}
              <li>
                <div>{{company}}</div>
                <div>{{title}}</div>
                <div>{{startDate}} - {{endDate}}</div>
                <div class="mb-4">
                  <ul>
                    {{#each responsibilities}}
                    <li><q> {{this}} </q></li>
                    {{/each}}
                  </ul>
                </div>
                <div>{{year_founded}}</div>
                <div>{{industry}}</div>
                <div>
                  <ul>
                    {{#each employees}}
                    <li>
                      <div>{{name}}</div>
                      <div>{{position}}</div>
                      <div>{{department}}</div>
                      <div>
                        <ul>
                          {{#each projects}}
                          <li>
                            <div>{{name}}</div>
                            <div>
                              <p>
                                {{description}}
                              </p>
                            </div>
                            <div>{{status}}</div>
                            <div>
                              <ul>
                                {{#each teams}}
                                <li>
                                  <div>{{team_name}}</div>
                                  <div>{{team_leader.name}}</div>
                                  <div>{{team_leader.title}}</div>
                                  <div>{{team_leader.email}}</div>
                                  <div>
                                    <ul>
                                      {{#each
                                      team_members
                                      }}
                                      <li>
                                        <div>{{name}}</div>
                                        <div>{{title}}</div>
                                        <div>{{email}}</div>
                                        <div>
                                          <ul>
                                            {{#each
                                            skills
                                            }}
                                            <li>
                                              <div>{{this}}</div>
                                            </li>
                                            {{/each}}
                                          </ul>
                                        </div>
                                        <div>
                                          <ul>
                                            {{#each
                                            projects
                                            }}
                                            <li>
                                              <div>
                                                {{name}}
                                              </div>
                                              <div>
                                                <p>{{description}}</p>
                                              </div>
                                              <div>
                                                <ul>
                                                  {{#each
                                                  technologies
                                                  }}
                                                  <li>
                                                    <div>{{this}}</div>
                                                  </li>
                                                  {{/each}}
                                                </ul>
                                              </div>
                                              <div>
                                                <ul>
                                                  {{#each
                                                  team_members
                                                  }}
                                                  <li>
                                                    <div>{{name}}</div>
                                                    <div>{{title}}</div>
                                                  </li>
                                                  {{/each}}
                                                </ul>
                                              </div>
                                            </li>
                                            {{/each}}
                                          </ul>
                                        </div>
                                      </li>
                                      {{/each}}
                                    </ul>
                                  </div>
                                </li>
                                {{/each}}
                              </ul>
                            </div>
                          </li>
                          {{/each}}
                        </ul>
                      </div>
                    </li>
                    {{/each}}
                  </ul>
                </div>
              </li>
              {{/each}}
            </ul>
          </section>
        </div>
      </div>
    </div>
  </div>
  {{/with}}
</main>