<nav>
  <ol class='breadcrumb'>
    {{#each breadcrumb}}
      {{#unless @last}}
        <li class='breadcrumb-item'>
          <a class='text-decoration-none' href={{url}}>
            {{name}}
          </a>
        </li>
      {{else}}
        <li class='breadcrumb-item active'>
          {{name}}
        </li>
      {{/unless}}
    {{/each}}
  </ol>
</nav>